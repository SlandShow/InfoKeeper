<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test_apps.slandshow.infokeeper">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_infokeeper"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EntryManager" />
        <activity android:name=".EntryViewActivity" />
        <activity android:name=".EntryDestroyer" />
        <activity android:name=".PasswordGenerator"></activity>
    </application>

</manifest>